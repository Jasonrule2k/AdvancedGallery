<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
  package="org.wit.gallery">

  <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

  <application
    android:name="org.wit.gallery.main.MainApp"
    android:allowBackup="true"
    android:icon="@mipmap/ic_launcher"
    android:label="@string/app_name"
    android:roundIcon="@mipmap/ic_launcher_round"
    android:supportsRtl="true"
    android:theme="@style/AppTheme">

    <meta-data
      android:name="com.google.android.geo.IzaSyBpA2upAS1xTbuHlJWXsw-R07G4zN_55cM"
      android:value="@string/google_maps_key" />

    <activity
      android:name="org.wit.gallery.activities.MapsActivity"
      android:label="@string/title_activity_maps2"></activity>
    <activity android:name=".activities.GallerySpinner"/>
    <activity android:name="org.wit.gallery.activities.GalleryActivity" />
    <activity android:name="org.wit.gallery.activities.GalleryListActivity">
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />

        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>

    <activity
      android:name="org.wit.gallery.activities.GalleryMapsActivity"
      android:label="@string/title_activity_gallery_maps"
      android:theme="@style/AppTheme" />
  </application>

</manifest>